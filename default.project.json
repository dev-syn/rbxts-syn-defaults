{
	"name": "syn_defaults",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "Folder",
		"syn-defaults": { "$path": "out" },
		"rbxts_include": {
      "$path": "include",
      "node_modules": {
        "$className": "Folder",
        "@rbxts": {
          "$path": "node_modules/@rbxts"
        }
      }
    }
	}
}