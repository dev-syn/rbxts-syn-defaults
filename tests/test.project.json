{
	"name": "syn-defaults",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",

		"ReplicatedStorage": {
			"rbxts_include": {
        "$path": "../include",
        "node_modules": {
          "$className": "Folder",
          "@rbxts": {
            "$path": "../node_modules/@rbxts"
          }
        }
      },
			"syn-defaults": {
				"$path": "../out/src",

				"App": { "$path": "../out/tests/App.luau" },
				"SelectableGroup": { "$path": "../out/tests/SelectableGroup.luau" },
				"SampleToolTip": { "$path": "../out/tests/SampleToolTip.luau" },
				"SampleContextMenu": { "$path": "../out/tests/SampleContextMenu.luau" }
			}
		},
		"StarterPlayer": {
			"StarterPlayerScripts": {
				"testRunner": { "$path": "../out/tests/testRunner.client.luau" }
			}
		}
	}
}